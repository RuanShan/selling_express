<div class="page-header">
  <%= link_to aws_responses_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_aws_response_path(@aws_response), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show aws_response</h1>
</div>

<dl class="dl-horizontal">
  <dt>Amazon request:</dt>
  <dd><%= @aws_response.amazon_request_id %></dd>

  <dt>Next token:</dt>
  <dd><%= @aws_response.next_token %></dd>

  <dt>Request type:</dt>
  <dd><%= @aws_response.request_type %></dd>

  <dt>Page num:</dt>
  <dd><%= @aws_response.page_num %></dd>

  <dt>Last updated before:</dt>
  <dd><%= @aws_response.last_updated_before %></dd>

  <dt>Created before:</dt>
  <dd><%= @aws_response.created_before %></dd>

</dl>
